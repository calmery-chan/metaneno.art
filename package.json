{
  "dependencies": {
    "next": "9.5.3",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@types/node": "14.11.2",
    "@types/react": "16.9.50",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "@vercel/node": "1.8.3",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-react": "7.21.3",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "npm-run-all": "4.1.5",
    "sass": "1.26.11",
    "stylelint": "13.7.2",
    "stylelint-config-standard": "20.0.0",
    "svgo": "1.3.2",
    "tailwindcss": "1.8.10",
    "typescript": "4.0.3",
    "vercel": "20.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged -c lint-staged.config.js"
    }
  },
  "lint-staged": {
    "**/*": [
      "npm run fix"
    ]
  },
  "name": "metaneno",
  "private": true,
  "scripts": {
    "fix": "run-s fix:eslint fix:prettier fix:stylelint fix:svgo",
    "fix:eslint": "eslint --ext .js,.ts,.tsx --fix --ignore-path .gitignore .",
    "fix:prettier": "prettier --ignore-path .gitignore --write .",
    "fix:stylelint": "stylelint --fix --ignore-path .gitignore **/*.scss",
    "fix:svgo": "svgo **/*.svg",
    "start": "vercel dev"
  },
  "version": "0.1.0"
}
